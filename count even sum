import java.io.*;
import java.util.*;
class Count{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int sum=0,count=0,r;
        for(int i=1;i<=N;i++){
        sum=0;
        int b=i;
        while(b>0){
            r=b%10;
            sum=sum+r;
            b=b/10;
        }
        if(sum%2==0){
            count++;
        }
        
    }
    System.out.println("count is "+count);
    }
}
